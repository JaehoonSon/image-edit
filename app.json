{
  "expo": {
    "name": "Elysia - Photo Glow",
    "slug": "elysia",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#faeeda"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "appStoreUrl": "https://apps.apple.com/us/app/elysia-photo-glow/id6749180620",
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "buildeNumber": "2",
      "bundleIdentifier": "com.theblucks.glowly",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "We need to use your camera to let you take photos or scan items.",
        "NSPhotoLibraryUsageDescription": "We need to access your photo library so you can choose images from your gallery.",
        "NSPhotoLibraryAddUsageDescription": "We need permission to save images to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.knuceles.ExpoTemplate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-video",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "knuceles",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a7e567b5-0040-4b61-8bc8-64963c8b4465"
      }
    }
  }
}
